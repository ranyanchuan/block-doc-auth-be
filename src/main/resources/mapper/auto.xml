<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" ><mapper namespace="com.yyan.dao.AuthDao">    <!--插入一个区块-->    <insert id="insertAuth" parameterType="com.yyan.pojo.Auth">        <!-- mysql插入数据后，获取id -->        insert into auth(id,userId,docId, departmentId,state,sTime,eTime,createTime,updateTime) values        (uuid(),#{userId},#{docId},#{departmentId},#{state} #{sTime},#{eTime},now(), now())    </insert>    <!--修改-->    <update id="updateAuth" parameterType="com.yyan.pojo.Auth">        update auth        <set>            <if test="state!=null">state=#{state},</if>            updateTime=now()        </set>        where 1=1 and id = #{id}    </update></mapper>