<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" ><mapper namespace="com.yyan.dao.DepartmentDao">    <!--插入一条数据,返回 id-->    <insert id="insertDepartment" parameterType="com.yyan.pojo.Department">        <!-- mysql插入数据后，获取id -->        insert into department(id, parentId,title,createTime,updateTime) values        (#{id}, #{parentId},#{title}, now(), now())    </insert>    <!--查询-->    <select id="selectListDepartment" resultType="com.yyan.pojo.Department">        SELECT * FROM department WHERE 1=1    </select>    <!--查询-->    <select id="getDepartmentById" parameterType="java.lang.String" resultType="com.yyan.pojo.Department">        SELECT * FROM department WHERE id=#{id}    </select></mapper>