<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" ><mapper namespace="com.yyan.dao.CommentDao">    <!--插入一个区块-->    <insert id="insertComment" parameterType="com.yyan.pojo.Comment">        <!-- mysql插入数据后，获取id -->        insert into comment(id, title,userId,docId,createTime,updateTime) values        (#{id}, #{title},#{userId},#{docId},#{createTime}, #{updateTime})    </insert></mapper>